{
  "name": "Chrome Extension Example Action Page",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Search OpenBase",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "storage"
  ]
}
