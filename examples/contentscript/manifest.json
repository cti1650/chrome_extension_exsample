{
  "name": "Chrome Extension Example Content Script",
  "version": "0.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://www.npmjs.com/package/*",
    "https://github.com/*"
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "scripting"
  ]
}
